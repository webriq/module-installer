{
    "name": "webriqcms/module-installer",
    "type": "composer-installer",
    "description": "Composer installer for types: webriqcms-module & webriqcms-modules",
    "license": "BSD-3-Clause",
    "keywords": [
        "webriqcms",
        "composer-installer",
        "webriqcms-module",
        "webriqcms-modules"
    ],
    "homepage": "http://webriq.com/",
    "authors": [
        {
            "name": "WebriQ Dev Team",
            "email": "info@webriq.com",
            "homepage": "http://webriq.com/",
            "role": "Development Team"
        }
    ],
    "support": {
        "email": "info@webriq.com",
        "source": "https://github.com/webriq/module-installer"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "composer/composer": "*@dev"
    },
    "autoload": {
        "psr-0": {
            "Grid\\Installer": "src/"
        }
    },
    "extra": {
        "class": "Grid\\Installer\\ModuleInstaller"
    }
}
